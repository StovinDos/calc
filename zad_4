#include <stdio.h>
#include <math.h>


int get_int_len(int value) {
	int l = 1;
	while (value > 9) { l++; value /= 10; }
	return l;
}

int main() {
   
	int N, K, B;
	int temp, temp1;

	scanf_s("%d", &N);
	temp = N;
	N >= 0 ? N : -N;
	scanf_s("%d", &K);
	K = get_int_len(N) - K;
	scanf_s("%d", &B);

	for (size_t i = 0; i <  K; i++) {
	    N = N / 10;
	}
	N = N % 10;
	printf("\n%d\n", N);

	while (temp =! '\0') {
         
		
		printf("%d", temp1);

	}
}
